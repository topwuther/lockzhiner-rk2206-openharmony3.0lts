{
  "app": {
    "bundleName": "com.ohos.camera",
    "vendor": "ohos",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 6,
      "target": 6,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.ohos.camera",
    "name": ".MyApplication",
    "deviceType": [
      "phone",
      "tv",
      "wearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "visible": true,
        "name": "com.ohos.camera.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "srcPath": "default",
        "srcLanguage": "js"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/PreviewView/PreviewView",
          "pages/DistributedView/DistributedView"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}